{
	"name": "election",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"run:browser": "live-server --entry-file=index.html public",
		"build:browser:debug:html": "html-minifier --input-dir src/browser/html --output-dir public",
		"build:browser:debug:webpack": "webpack --config config/webpack.web.js",
		"build:browser:debug:sass": "node-sass --include-path src/browser/sass src/browser/sass/style.scss public/style.css",
		"build:browser:debug": "concurrently -r \"npm run build:browser:debug:html\" \"npm run build:browser:debug:webpack\" \"npm run build:browser:debug:sass\"",
		"build:browser:prod:html": "html-minifier -c config/htmlmin.json --input-dir src/browser/html --output-dir public",
		"build:browser:prod:webpack": "webpack -p --config config/webpack.web.prod.js",
		"build:browser:prod:sass": "node-sass --output-style compressed --include-path src/browser/sass src/browser/sass/style.scss public/style.css",
		"build:browser:prod": "concurrently -r \"npm run build:browser:prod:html\" \"npm run build:browser:prod:webpack\" \"npm run build:browser:prod:sass\"",
		"watch:browser:debug:webpack": "webpack -w --config config/webpack.web.js",
		"watch:browser:debug:html": "watch \"npm run build:browser:debug:html\" src/browser/html",
		"watch:browser:debug:sass": "node-sass -w --include-path src/browser/sass src/browser/sass/style.scss public/style.css",
		"watch:browser:debug": "concurrently -rk \"npm run watch:browser:debug:webpack\" \"npm run watch:browser:debug:html\" \"npm run watch:browser:debug:sass\"",
		"watch:browser:prod:webpack": "webpack -wp --config config/webpack.web.prod.js",
		"watch:browser:prod:html": "watch \"npm run build:browser:prod:html\" src/browser/html",
		"watch:browser:prod:sass": "node-sass -w --output-style compressed --include-path src/browser/sass src/browser/sass/style.scss public/style.css",
		"watch:browser:prod": "concurrently -rk \"npm run watch:browser:prod:webpack\" \"npm run watch:browser:prod:html\" \"npm run watch:browser:prod:sass\"",
		"start:browser:debug": "npm run build:browser:debug && concurrently -rk \"npm run watch:browser:debug\" \"npm run run:browser\"",
		"start:browser:prod": "npm run build:browser:prod && concurrently -rk \"npm run watch:browser:prod\" \"npm run run:browser\"",
		"start:debug": "concurrently -rk \"npm run start:browser:debug\"",
		"start:prod": "concurrently -rk \"npm run start:browser:prod\"",
		"build:prod": "npm run build:browser:prod",
		"start": "npm run start:debug"
	},
	"dependencies": {
		"@types/d3": "^4.7.0",
		"@types/isomorphic-fetch": "^0.0.33",
		"@types/node": "^7.0.12",
		"@types/react": "^15.0.21",
		"@types/react-dom": "^0.14.23",
		"awesome-typescript-loader": "^3.1.2",
		"babel-core": "^6.24.1",
		"babel-polyfill": "^6.23.0",
		"babel-preset-es2015": "^6.24.1",
		"concurrently": "^3.4.0",
		"d3": "^4.7.4",
		"html-minifier": "^3.4.3",
		"isomorphic-fetch": "^2.2.1",
		"json-loader": "^0.5.4",
		"live-server": "^1.2.0",
		"node-sass": "^4.5.2",
		"react": "^15.5.4",
		"react-dom": "^15.5.4",
		"regenerator-runtime": "^0.10.3",
		"shallowequal": "^0.2.2",
		"topojson": "^3.0.0",
		"tslint": "^5.1.0",
		"tslint-react": "^2.5.0",
		"typescript": "^2.2.2",
		"watch": "^1.0.2",
		"webpack": "^2.3.3"
	}
}
